# EditorConfig is awesome: https://EditorConfig.org

[*]
end_of_line = lf
trim_trailing_whitespace = true
charset = utf-8
indent_style = space
indent_size = 2
insert_final_newline = false

[*.sh]
indent_style = tab
insert_final_newline = true

[{*.md,*.mkd}]
indent_style = tab
insert_final_newline = true

